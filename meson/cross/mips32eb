# See http://mesonbuild.com/Cross-compilation.html

[binaries]
c = 'mips-helenos-gcc'
cpp = 'mips-helenos-g++'
ar = 'mips-helenos-ar'
strip = 'mips-helenos-strip'
nm = 'mips-helenos-nm'
objcopy = 'mips-helenos-objcopy'
objdump = 'mips-helenos-objdump'
as = 'mips-helenos-as'
ld = 'mips-helenos-ld'

[properties]
has_function_printf = true
needs_exe_wrapper = true

cc_arch = 'mips'
h_arch = 'mips32'
bits = 32

[host_machine]
system = 'helenos'
cpu_family = 'mips'
cpu = 'mips'
endian = 'big'
