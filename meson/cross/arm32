[binaries]
c = 'arm-helenos-gcc'
cpp = 'arm-helenos-g++'
ar = 'arm-helenos-ar'
strip = 'arm-helenos-strip'
nm = 'arm-helenos-nm'
objcopy = 'arm-helenos-objcopy'
objdump = 'arm-helenos-objdump'
as = 'arm-helenos-as'
ld = 'arm-helenos-ld'

[properties]
has_function_printf = true
needs_exe_wrapper = true

cc_arch = 'arm'
bits = 32

[host_machine]
system = 'helenos'
cpu_family = 'arm'
cpu = 'arm'
endian = 'little'
